    "deployhost": "npm run build && firebase deploy --only hosting",
    "logs": "firebase functions:log",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore .",
    "storybook": "start-storybook -p 6006 -c .storybook"
