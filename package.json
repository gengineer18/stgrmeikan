{
  "name": "stgrmeikan-next",
  "version": "0.1.0",
  "description": "Host Next.js SSR app on Firebase Cloud Functions with Firebase Hosting redirects.",
  "engines": {
    "node": "12"
  },
  "license": "UNLICENSED",
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
